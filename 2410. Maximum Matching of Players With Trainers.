class Solution {
    public int matchPlayersAndTrainers(int[] players, int[] trainers) {
        Arrays.sort(players);
        Arrays.sort(trainers);
        int size = 0, count = 0;
        for(int i = 0;i < trainers.length;i++) {
            int temp = trainers[i];
            if(!(size >= players.length)) {
                if(temp >= players[size]) {
                    count++;size++;
                }
            }
        }
        return count;
    }
}
